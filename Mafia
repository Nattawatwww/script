-- CHARACTER SPAWN
--==================================================
player.CharacterAdded:Connect(function(char)
    noclip = false
    flying = false
    DisableNoClip()
    DisableLockVelocity()
    originalState = {}
    removeNoclipDetector(char)
	BindAutoCrateDeathTeleport(char)
end)

if player.Character then
    removeNoclipDetector(player.Character)
end

task.spawn(function()
    while true do
        if ESPEnabled then
            RefreshESP()
        end
        task.wait(0.5)
    end
end)

updateAll()
LOOT_PATH.ChildAdded:Connect(updateAll)
LOOT_PATH.ChildRemoved:Connect(updateAll)
