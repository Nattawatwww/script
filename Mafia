local CrateUI = Tab:AddParagraph({
    Title = "Weapon Crate",
    Content = "à¸à¸³à¸¥à¸±à¸‡à¸•à¸£à¸§à¸ˆà¸ªà¸­à¸š..."
})

local function GetCrateCount()
    local count = 0
    for _, v in ipairs(LOOT_PATH:GetChildren()) do
        if v.Name == "WeaponCrate" then
            count += 1
        end
    end
    return count
end

local function UpdateCrateUI()
    local count = GetCrateCount()

    if count == 0 then
        CrateUI:SetDesc("âŒ WeaponCrate : 0")
    else
        CrateUI:SetDesc("ðŸ“¦ WeaponCrate : " .. count)
    end
end

local function applyHighlight(crate)
    if crate:FindFirstChild("CrateHighlight") then return end

    local h = Instance.new("Highlight")
    h.Name = "CrateHighlight"
    h.FillColor = Color3.fromRGB(255, 0, 0)
    h.FillTransparency = 0.5
    h.OutlineColor = Color3.fromRGB(255, 80, 80)
    h.OutlineTransparency = 0
    h.Adornee = crate
    h.Parent = crate
end

local function removeHighlight(crate)
    local h = crate:FindFirstChild("CrateHighlight")
    if h then
        h:Destroy()
    end
end

local function UpdateCrateUI()
    local count = 0

    for _, v in ipairs(LOOT_PATH:GetChildren()) do
        if v.Name == "WeaponCrate" then
            count += 1
            applyHighlight(v)
        end
    end

    if count == 0 then
        CrateUI:SetDesc("âŒ WeaponCrate : 0")
    else
        CrateUI:SetDesc("ðŸ“¦ WeaponCrate : " .. count)
    end
end

UpdateCrateUI()
LOOT_PATH.ChildAdded:Connect(UpdateCrateUI)
LOOT_PATH.ChildRemoved:Connect(UpdateCrateUI)


--==================================================
-- PLAYER ESP
--==================================================
local highlightFolder = Workspace:FindFirstChild("HighlightContainer") or Instance.new("Folder")
highlightFolder.Name = "HighlightContainer"
highlightFolder.Parent = Workspace

local function getTeamColor(player)
    if not player.Team or not localPlayer.Team then
        return Color3.fromRGB(255,0,0)
    end
    return (player.Team == localPlayer.Team)
        and Color3.fromRGB(0,170,255)
        or Color3.fromRGB(255,0,0)
end

local function applyESP(p)
    if not ESPEnabled then return end
    if p == player then return end

    local function setup(character)
        if not ESPEnabled then return end
        if p == player then return end

        local old = highlightFolder:FindFirstChild(p.Name .. "_ESP")
        if old then old:Destroy() end

        local h = Instance.new("Highlight")
        h.Name = p.Name .. "_ESP"
        h.Adornee = character
        h.FillColor = getTeamColor(p)
        h.OutlineColor = Color3.new(1,1,1)
        h.OutlineTransparency = 0.2
        h.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
        h.Parent = highlightFolder
    end

    p.CharacterAdded:Connect(setup)
    if p.Character then
        setup(p.Character)
    end
end

local function ClearESP()
    for _,v in ipairs(highlightFolder:GetChildren()) do
        v:Destroy()
    end
end

local function RefreshESP()
    if not ESPEnabled then return end

    ClearESP()

    for _,p in ipairs(Players:GetPlayers()) do
        if p ~= player and p.Character then
            applyESP(p)
        end
    end
end

--==================================================
-- AIMLOCK PLAYER
--==================================================

local function IsVisible(targetPart, useCheck)
    if not useCheck then
        return true
    end

    local origin = Camera.CFrame.Position
    local direction = targetPart.Position - origin

    local params = RaycastParams.new()
    params.FilterType = Enum.RaycastFilterType.Blacklist
    params.FilterDescendantsInstances = {
        player.Character,
        Camera
    }

    local result = Workspace:Raycast(origin, direction, params)
    return result and result.Instance:IsDescendantOf(targetPart.Parent)
end

local PlayerLocking = false
local PlayerSettings = {
    Key = Enum.KeyCode.E,
    FOV = 60,
    Smooth = 0.4,
    AimPart = "Head",
    VisibleCheck = true
}

local PlayerFOV = Drawing.new("Circle")
PlayerFOV.Visible = false
PlayerFOV.Thickness = 2
PlayerFOV.Radius = PlayerSettings.FOV
PlayerFOV.Color = Color3.new(1,1,1)
PlayerFOV.Filled = false

local function GetClosestPlayer()
    local closest, dist = nil, PlayerSettings.FOV
    local center = Vector2.new(
        Camera.ViewportSize.X / 2,
        Camera.ViewportSize.Y / 2
    )

    for _,p in ipairs(Players:GetPlayers()) do
        if p ~= player and p.Character then
            local part = p.Character:FindFirstChild(PlayerSettings.AimPart)
            local hum = p.Character:FindFirstChild("Humanoid")

            if part and hum and hum.Health > 0 then
                local pos, onScreen =
                    Camera:WorldToViewportPoint(part.Position)

                if onScreen then
                    local d = (center - Vector2.new(pos.X, pos.Y)).Magnitude
                    if d < dist and IsVisible(part, PlayerSettings.VisibleCheck) then
                        closest = part
                        dist = d
                    end
                end
            end
        end
    end

    return closest
end

--==================================================
-- AIMLOCK NPC / ENEMY
--==================================================
local NPCLocking = false
local NPCSettings = {
    Key = Enum.KeyCode.F,
    FOV = 60,
    Smooth = 0.4,
    AimPart = "Head",
    VisibleCheck = true
}

local TargetFolders = {
    Workspace:WaitForChild("Enemies"),
    Workspace.Map.MapModel.JewelryStore:WaitForChild("Npcs"),
    Workspace.Map.MapModel.CargoShip:WaitForChild("Npcs"),
    Workspace.Map:WaitForChild("Civilians")
}

local NPCFOV = Drawing.new("Circle")
NPCFOV.Visible = false
NPCFOV.Radius = NPCSettings.FOV
NPCFOV.Thickness = 2
NPCFOV.Color = Color3.new(1,1,1)

local function GetClosestNPC()
    local closest, dist = nil, NPCSettings.FOV
    local center = Vector2.new(
        Camera.ViewportSize.X / 2,
        Camera.ViewportSize.Y / 2
    )

    for _,folder in ipairs(TargetFolders) do
        for _,m in ipairs(folder:GetChildren()) do
            if m:IsA("Model") then
                local hum = m:FindFirstChildOfClass("Humanoid")
                local part = m:FindFirstChild(NPCSettings.AimPart)

                if hum and part and hum.Health > 0 then
                    local pos, onScreen =
                        Camera:WorldToViewportPoint(part.Position)

                    if onScreen then
                        local d = (center - Vector2.new(pos.X, pos.Y)).Magnitude
                        if d < dist and IsVisible(part, NPCSettings.VisibleCheck) then
                            closest = part
                            dist = d
                        end
                    end
                end
            end
        end
    end
    return closest
end

UIS.InputBegan:Connect(function(input,gp)
    if gp then return end

    if AimPlayerEnabled and input.KeyCode == PlayerSettings.Key then
        PlayerLocking = true
    end

    if AimNPCEnabled and input.KeyCode == NPCSettings.Key then
        NPCLocking = true
    end
end)

UIS.InputEnded:Connect(function(input)
    if input.KeyCode == PlayerSettings.Key then
        PlayerLocking = false
    end
    if input.KeyCode == NPCSettings.Key then
        NPCLocking = false
    end
end)

RunService.RenderStepped:Connect(function()
    local center = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)

    PlayerFOV.Visible = AimPlayerEnabled
    NPCFOV.Visible = AimNPCEnabled
    PlayerFOV.Position = center
    NPCFOV.Position = center

    if AimPlayerEnabled and PlayerLocking then
        local t = GetClosestPlayer()
        if t then
            Camera.CFrame = Camera.CFrame:Lerp(
                CFrame.lookAt(Camera.CFrame.Position, t.Position),
                PlayerSettings.Smooth
            )
        end
    end

    if AimNPCEnabled and NPCLocking then
        local t = GetClosestNPC()
        if t then
            Camera.CFrame = Camera.CFrame:Lerp(
                CFrame.lookAt(Camera.CFrame.Position, t.Position),
                NPCSettings.Smooth
            )
        end
    end
end)

local function TeleportRS()
    local url =
        "https://games.roblox.com/v1/games/"
        .. PlaceId
        .. "/servers/Public?limit=100"

    local ok, res = pcall(function()
        return HttpService:JSONDecode(game:HttpGet(url))
    end)

    if not ok or not res or not res.data then
        warn("âŒ à¹‚à¸«à¸¥à¸”à¸£à¸²à¸¢à¸Šà¸·à¹ˆà¸­à¹€à¸‹à¸´à¸£à¹Œà¸Ÿà¹€à¸§à¸­à¸£à¹Œà¹„à¸¡à¹ˆà¹„à¸”à¹‰")
        return
    end

    local servers = {}

    for _, s in ipairs(res.data) do
        if s.id ~= game.JobId and s.playing < s.maxPlayers then
            table.insert(servers, s.id)
        end
    end

    if #servers == 0 then
        warn("âŒ à¹„à¸¡à¹ˆà¸¡à¸µà¹€à¸‹à¸´à¸£à¹Œà¸Ÿà¹€à¸§à¸­à¸£à¹Œà¹ƒà¸«à¹‰à¸ªà¸¸à¹ˆà¸¡")
        return
    end

    -- ðŸ”€ à¸ªà¸¸à¹ˆà¸¡à¸¥à¸³à¸”à¸±à¸š
    for i = #servers, 2, -1 do
        local j = math.random(i)
        servers[i], servers[j] = servers[j], servers[i]
    end

    local tries = math.min(3, #servers)

    for i = 1, tries do
        local success = pcall(function()
            TeleportService:TeleportToPlaceInstance(
                PlaceId,
                servers[i],
                player
            )
        end)

        if success then
            return
        end
    end

    -- ðŸ›Ÿ à¸à¸±à¸™à¸žà¸¥à¸²à¸”à¸ˆà¸£à¸´à¸‡
    TeleportService:Teleport(PlaceId, player)
end
--==================================================
-- ADVANCED NOCLIP + STATE
--==================================================
local noclip = false
local flying = false
local noclipConn
local originalState = {}

local function SaveState(character)
    originalState = {}
    for _, part in ipairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            originalState[part] = {
                CanCollide = part.CanCollide,
                CanTouch   = part.CanTouch,
                CanQuery   = part.CanQuery
            }
        end
    end
end

local function RestoreState()
    for part, state in pairs(originalState) do
        if part and part.Parent then
            part.CanCollide = state.CanCollide
            part.CanTouch   = state.CanTouch
            part.CanQuery   = state.CanQuery
        end
    end
    originalState = {}
end

local function EnableNoClip(character)
    if noclipConn then noclipConn:Disconnect() end
    noclipConn = RunService.Heartbeat:Connect(function()
        for _, part in ipairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
                part.CanTouch   = false
                part.CanQuery   = false
            end
        end
    end)
end

local function DisableNoClip()
    if noclipConn then
        noclipConn:Disconnect()
        noclipConn = nil
    end
end

--==================================================
-- FLY
--==================================================
local function SmoothFly(targetCFrame, speed)
    if flying then return end
    flying = true

    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    SaveState(char)
    EnableNoClip(char)

    local velConn
    velConn = RunService.Heartbeat:Connect(function()
        if hrp and hrp.Parent then
            hrp.Velocity = Vector3.zero
            hrp.AssemblyLinearVelocity = Vector3.zero
        end
    end)

    local startPos = hrp.Position
    local targetY = targetCFrame.Position.Y
    local tY = math.abs(startPos.Y - targetY) / speed

    local tweenY = TweenService:Create(
        hrp,
        TweenInfo.new(tY, Enum.EasingStyle.Linear),
        { CFrame = CFrame.new(startPos.X, targetY, startPos.Z) * hrp.CFrame.Rotation }
    )
    tweenY:Play()
    tweenY.Completed:Wait()

    local dist = (hrp.Position - targetCFrame.Position).Magnitude
    local tMove = dist / speed

    local tweenMove = TweenService:Create(
        hrp,
        TweenInfo.new(tMove, Enum.EasingStyle.Linear),
        { CFrame = targetCFrame }
    )
    tweenMove:Play()
    tweenMove.Completed:Wait()

    if velConn then velConn:Disconnect() end
    DisableNoClip()
    RestoreState()
    flying = false
end

--==================================================
-- LOCK VELOCITY (AUTOCRATE ONLY)
--==================================================
local lockVelConn

local function EnableLockVelocity(hrp)
    if lockVelConn then lockVelConn:Disconnect() end
    lockVelConn = RunService.Heartbeat:Connect(function()
        if hrp and hrp.Parent then
            hrp.Velocity = Vector3.zero
            hrp.AssemblyLinearVelocity = Vector3.zero
        end
    end)
end

local function DisableLockVelocity()
    if lockVelConn then
        lockVelConn:Disconnect()
        lockVelConn = nil
    end
end

--==================================================
-- FlyAndStay
--==================================================

local stayFly = false
local stayConn

local function FlyAndStay(cf)
    if stayFly then return end
    stayFly = true

    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    SmoothFly(cf, 35)

    SaveState(char)
    EnableNoClip(char)
    EnableLockVelocity(hrp)

    stayConn = RunService.Heartbeat:Connect(function()
        if not stayFly then return end
        if hrp and hrp.Parent then
            hrp.CFrame = cf
            hrp.Velocity = Vector3.zero
            hrp.AssemblyLinearVelocity = Vector3.zero
        end
    end)
end

local function StopStayFly()
    stayFly = false

    if stayConn then
        stayConn:Disconnect()
        stayConn = nil
    end

    DisableLockVelocity()
    DisableNoClip()
    RestoreState()
end

local function LookUp()
    local cam = workspace.CurrentCamera
    local pos = cam.CFrame.Position

    cam.CFrame = CFrame.lookAt(
        pos,
        pos + Vector3.new(0, 1, 0)
    )
end

local function BindAutoCrateDeathTeleport(character)
    local hum = character:WaitForChild("Humanoid", 5)
    if not hum then return end

    hum.Died:Connect(function()
        if not autoCrate then return end
        autoCrate = false
        autoCrateReady = false

        DisableLockVelocity()
        DisableNoClip()
        RestoreState()

        Fluent:Notify({
            Title = "Auto Crate",
            Content = "à¸„à¸¸à¸“à¸•à¸²à¸¢à¹à¸¥à¹‰à¸§ à¸à¸³à¸¥à¸±à¸‡à¸¢à¹‰à¸²à¸¢à¹€à¸‹à¸´à¸£à¹Œà¸Ÿà¹€à¸§à¸­à¸£à¹Œ...",
            Duration = 4
        })

        task.wait(1)
        TeleportRS()
    end)
end

local function AutoCrateLoop()
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    while autoCrate do
        autoCrateReady = true

        local weaponCrateInChar = char:FindFirstChild("PlayerWeaponCrate")

        if GetCrateCount() == 0 and not weaponCrateInChar then
            TeleportRS()
            return
        end

        if GetCrateCount() == 0 and weaponCrateInChar and not waitingTeleport then
            waitingTeleport = true

            task.spawn(function()
                while autoCrate and char:FindFirstChild("PlayerWeaponCrate") do
                    task.wait(0.2)
                end

                if autoCrate then
                    task.wait(3)
                end

                if autoCrate and GetCrateCount() == 0 then
                    TeleportRS()
                end

                waitingTeleport = false
            end)
        end

        for _, crate in ipairs(LOOT_PATH:GetChildren()) do
            if not autoCrate then break end
            if crate.Name ~= "WeaponCrate" then continue end

            local part = crate:FindFirstChildWhichIsA("BasePart")
            local prompt = crate:FindFirstChildWhichIsA("ProximityPrompt", true)

            if part and prompt and crate.Parent then
                EnableLockVelocity(hrp)
                EnableNoClip(char)

                while autoCrate and crate.Parent do
                    hrp.CFrame = part.CFrame * CFrame.new(0, -5, 0)
                    task.wait(0.09)
                    pcall(function()
                        fireproximityprompt(prompt)
                    end)
                    task.wait(0.03)
                    if char:FindFirstChild("PlayerWeaponCrate") then
                        repeat
                            hrp.CFrame = TARGET_2
                            task.wait(0.5)
                        until not char:FindFirstChild("PlayerWeaponCrate")
                        break
                    end
                end

                DisableLockVelocity()
                DisableNoClip()
                RestoreState()
            end
        end
    end
end

